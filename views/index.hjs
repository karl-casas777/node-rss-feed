<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.css">
        <link rel="stylesheet" href="/stylesheets/rss.css">
        <link rel="stylesheet" href="/bower_components/fontawesome/css/font-awesome.min.css">
        <title>RSS Reader</title>
    </head>
    <body>
        <div class="container-fluid header">
            <div class="row">
                <img class="col-xs-1 col-xs-offset-2 padding-less" src="/images/ux.png" alt="ux">
                <div class="col-xs-6 searchbox input-group">
                    <span class="input-group-addon search-icon"><i class="fa fa-search"></i></span>
                    <input class="rss-search form-control" type="text">
                </div>
            </div>
        </div>
        <div class="container-fluid padding-less">
            <ul class="list-unstyled rss-list">
            </ul>
            <div class="row">
                <a class="load-more redlink col-xs-7 col-xs-offset-2 padding-less" href="">
                    <i class="fa fa-refresh fa-spin"></i>
                    Load More
                </a>
            </div>
        </div>
        <script src="/bower_components/jquery/dist/jquery.min.js"></script>
        <script src="socket.io/socket.io.js"></script>
        <script src="/javascripts/main.js"></script>
    </body>
</html>